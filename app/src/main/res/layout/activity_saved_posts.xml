<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    android:id="@+id/saved_post_activity"
    android:background="@color/whitesmoke_off_white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Profile.SavedPosts">


    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/rlMainContent"
        android:layout_marginBottom="55dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvSavedPosts"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
    <include layout="@layout/toolbar_saved_post" />

</RelativeLayout>